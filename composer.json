{
    "name": "tzurbaev/laravel-extra-fields-validator",
    "description": "Extra fields validator for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tzurbaev",
            "email": "zurbaev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\ExtraFieldsValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix": "php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": ["Laravel\\ExtraFieldsValidator\\ExtraFieldsValidatorServiceProvider"]
        }
    }
}
